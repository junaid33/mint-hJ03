{
  "name": "@mintlify/cli",
  "version": "3.0.8",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint",
    "directory": "packages/mintlify-cli"
  },
  "bugs": {
    "url": "https://github.com/mintlify/mint/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && node bin/index.js dev",
    "installDeps": "yarn build && node bin/index.js install",
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "jest",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@mintlify/previewing": "^3.1.3",
    "@mintlify/validation": "^0.1.9",
    "@octokit/rest": "^19.0.5",
    "axios": "^1.2.2",
    "chalk": "^5.1.0",
    "cheerio": "^0.22.0",
    "chokidar": "^3.5.3",
    "detect-port": "^1.5.1",
    "favicons": "^7.0.1",
    "fs-extra": "^11.1.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.1.0",
    "is-absolute-url": "^4.0.1",
    "is-internet-available": "^3.1.0",
    "minimist-lite": "^2.2.1",
    "node-html-markdown": "^1.3.0",
    "open": "^8.4.0",
    "openapi-types": "^12.0.2",
    "ora": "^6.1.2",
    "puppeteer": "^19.4.0",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.2.1",
    "shelljs": "^0.8.5",
    "unist-util-visit": "^4.1.1",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/ts-config": "1.0.7",
    "@tsconfig/recommended": "1.x",
    "@types/cheerio": "^0.22.31",
    "@types/detect-port": "^1.3.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.7.13",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "jest": "^29.4.3",
    "prettier": "2.x",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
